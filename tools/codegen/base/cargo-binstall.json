{
  "repository": "https://github.com/cargo-bins/cargo-binstall",
  "tag_prefix": "v",
  "rust_crate": "${package}",
  "asset_name": "${package}-${rust_target}.tgz",
  "version_range": "latest",
  "signing": {
    "kind": "minisign-binstall"
  },
  "platform": {
    "x86_64_linux_musl": {},
    "x86_64_macos": {
      "asset_name": "${package}-${rust_target}.zip"
    },
    "x86_64_windows": {
      "asset_name": "${package}-${rust_target}.zip"
    },
    "aarch64_linux_musl": {},
    "aarch64_macos": {
      "asset_name": "${package}-${rust_target}.zip"
    },
    "aarch64_windows": {
      "asset_name": "${package}-${rust_target}.zip"
    }
  }
}
