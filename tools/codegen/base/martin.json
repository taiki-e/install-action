{
  "repository": "https://github.com/maplibre/martin",
  "tag_prefix": "v",
  "rust_crate": "${package}",
  "asset_name": "${package}-${rust_target}.tar.gz",
  "version_range": ">= 0.11.0",
  "bin": ["martin", "martin-cp"],
  "platform": {
    "x86_64_linux_musl": {},
    "x86_64_macos": {},
    "x86_64_windows": {
      "asset_name": "${package}-${rust_target}.zip",
      "bin": ["martin.exe", "martin-cp.exe"]
    },
    "aarch64_linux_musl": {},
    "aarch64_macos": {}
  }
}
