{
  "repository": "https://github.com/maplibre/martin",
  "tag_prefix": "v",
  "rust_crate": "${package}",
  "asset_name": "${package}-${rust_target}",
  "version_range": ">= 0.11.0",
  "bin": [
    "martin${exe}",
    "martin-cp${exe}"
  ],
  "platform": {
    "x86_64_linux_musl": {
      "asset_name": "${package}-${rust_target}.tar.gz"
    },
    "x86_64_macos": {
      "asset_name": "${package}-${rust_target}.tar.gz"
    },
    "x86_64_windows": {
      "asset_name": "${package}-${rust_target}.zip"
    },
    "aarch64_linux_musl": {
      "asset_name": "${package}-${rust_target}.tar.gz"
    },
    "aarch64_macos": {
      "asset_name": "${package}-${rust_target}.tar.gz"
    }
  }
}
