{
  "repository": "https://github.com/cargo-lambda/cargo-lambda",
  "tag_prefix": "v",
  "rust_crate": "${package}",
  "asset_name": "${package}-v${version}.${rust_target}.tar.gz",
  "version_range": ">= 1.3.0",
  "broken": ["1.8.1"],
  "platform": {
    "x86_64_linux_musl": {},
    "x86_64_macos": {},
    "x86_64_windows": {
      "asset_name": "${package}-v${version}.windows-x64.zip",
      "bin": "${package}${exe}"
    },
    "aarch64_linux_musl": {},
    "aarch64_macos": {}
  }
}
