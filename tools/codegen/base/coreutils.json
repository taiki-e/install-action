  "tag_prefix": "",
  "rust_crate": "${package}",
  "asset_name": "${package}-${version}-${rust_target}.tar.gz",
  "bin": "${package}-${version}-${rust_target}/${package}${exe}",
  "version_range": ">= 0.1.0",
  "platform": {
    "x86_64_linux_musl": {},
    "x86_64_macos": {},
    "x86_64_windows": {
      "asset_name": "${package}-${version}-${rust_target}.zip"
    },
    "aarch64_linux_musl": {},
    "aarch64_macos": {},
    "aarch64_windows": {
      "asset_name": "${package}-${version}-${rust_target}.zip"
    }
  }
}
